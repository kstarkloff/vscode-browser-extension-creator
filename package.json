{
  "name": "browser-extension-creator",
  "displayName": "Browser Extension Creator",
  "description": "This extension creates a template for an extension of a browser of your choice.",
  "version": "0.0.1",
  "publisher": "StarkesWeb",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "browser-extension-creator.chrome",
        "title": "Create Chrome extension files"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "browser-extension-creator.chrome",
          "when": "!editorHasSelection",
          "group": "3_contributed"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "pngjs": "^7.0.0"
  }
}
